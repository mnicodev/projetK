{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/console": "^1.9",
        "drupal/core": "^8.7.0",
        "drupal/devel": "^2.1",
        "drupal/domain_role": "^1.0@alpha",
        "drupal/draggableviews": "^1.2",
        "drupal/druadmin_lte_theme": "^1.0",
        "drupal/drush": "^6.0@RC",
        "drupal/easy_breadcrumb": "^1.12",
        "drupal/entity_browser": "^2.5",
        "drupal/entity_reference_tree": "^1.8",
        "drupal/feeds": "^3.0@alpha",
        "drupal/field_group": "^1.0",
        "drupal/geolocation": "^3.2",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/kint": "^2.1",
        "drupal/limit_domain_access_by_role": "^1.5",
        "drupal/menu_breadcrumb": "^1.13",
        "drupal/menus_attribute": "^1.1",
        "drupal/metatag": "^1.13",
        "drupal/paragraphs": "^1.10",
        "drupal/pathauto": "^1.8",
        "drupal/taxonomy_delete": "^1.1",
        "drupal/taxonomy_import": "^1.0",
        "drupal/taxonomy_menu": "^3.4",
        "drupal/term_csv_export_import": "^3.0",
        "drupal/views_infinite_scroll": "^1.7",
        "drupal/weight": "^3.1",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.8.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
